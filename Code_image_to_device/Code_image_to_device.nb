Cell[CellGroupData[{

Cell["Function to create truncated cones", "Subsection",
 CellChangeTimes->{{3.906609335450378*^9, 
  3.9066093634514313`*^9}},ExpressionUUID->"7c88a281-d099-4262-b03e-\
6043ddcc123e"],

Cell["\<\
This function generates a 3 D model of a truncated cone based on the given \
parameters, base radius of the spine R, contact angle between liquid and \
3D-printed materials \[Theta], the (x,y) position and and the desired liquid \
elevation h. N represents the number of edges used to creates the side of the \
truncated cones. \
\>", "Text",
 CellChangeTimes->{{3.906609400715609*^9, 
  3.906609574323852*^9}},ExpressionUUID->"ccfb406a-cf75-4f9b-816a-\
f7f8f3318c56"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"conT", "[", 
    RowBox[{
    "x_", ",", "y_", ",", "R_", ",", "h_", ",", "\[Theta]_", ",", "N_"}], 
    "]"}], ":=", 
   RowBox[{"Translate", "[", 
    RowBox[{
     RowBox[{"ConvexHullMesh", "[", 
      RowBox[{"Flatten", "[", 
       RowBox[{
        RowBox[{"ParallelTable", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{
             RowBox[{"(", 
              RowBox[{"R", "-", 
               RowBox[{"R", "*", 
                RowBox[{"j", "/", 
                 RowBox[{"(", 
                  RowBox[{"h", " ", 
                   RowBox[{"R", "/", 
                    RowBox[{"(", 
                    RowBox[{"R", "-", 
                    RowBox[{"h", "/", 
                    RowBox[{"(", 
                    RowBox[{
                    RowBox[{"Cos", "[", "\[Theta]", "]"}], "*", 
                    RowBox[{"Sum", "[", 
                    RowBox[{
                    RowBox[{"Sum", "[", 
                    RowBox[{
                    RowBox[{"Exp", "[", 
                    RowBox[{
                    RowBox[{"-", 
                    RowBox[{"Sqrt", "[", 
                    RowBox[{
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{"R", "-", 
                    RowBox[{"k", " ", "a"}]}], ")"}], "^", "2"}], "+", 
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{"0", "-", 
                    RowBox[{"l", " ", "a"}]}], ")"}], "^", "2"}]}], "]"}]}], 
                    "/", "\[Lambda]"}], "]"}], ",", 
                    RowBox[{"{", 
                    RowBox[{"k", ",", 
                    RowBox[{"-", "12"}], ",", "12", ",", "1"}], "}"}]}], 
                    "]"}], ",", 
                    RowBox[{"{", 
                    RowBox[{"l", ",", 
                    RowBox[{"-", "12"}], ",", "12", ",", "1"}], "}"}]}], 
                    "]"}]}], ")"}]}]}], ")"}]}]}], ")"}]}]}]}], ")"}], "*", 
             RowBox[{"Cos", "[", 
              RowBox[{
               RowBox[{"360", "/", "N"}], "*", "i", " ", "Degree"}], "]"}]}], 
            ",", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"R", "-", 
               RowBox[{"R", "*", 
                RowBox[{"j", "/", 
                 RowBox[{"(", 
                  RowBox[{"h", " ", 
                   RowBox[{"R", "/", 
                    RowBox[{"(", 
                    RowBox[{"R", "-", 
                    RowBox[{"h", "/", 
                    RowBox[{"(", 
                    RowBox[{
                    RowBox[{"Cos", "[", "\[Theta]", "]"}], "*", 
                    RowBox[{"Sum", "[", 
                    RowBox[{
                    RowBox[{"Sum", "[", 
                    RowBox[{
                    RowBox[{"Exp", "[", 
                    RowBox[{
                    RowBox[{"-", 
                    RowBox[{"Sqrt", "[", 
                    RowBox[{
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{"R", "-", 
                    RowBox[{"k", " ", "a"}]}], ")"}], "^", "2"}], "+", 
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{"0", "-", 
                    RowBox[{"l", " ", "a"}]}], ")"}], "^", "2"}]}], "]"}]}], 
                    "/", "\[Lambda]"}], "]"}], ",", 
                    RowBox[{"{", 
                    RowBox[{"k", ",", 
                    RowBox[{"-", "12"}], ",", "12", ",", "1"}], "}"}]}], 
                    "]"}], ",", 
                    RowBox[{"{", 
                    RowBox[{"l", ",", 
                    RowBox[{"-", "12"}], ",", "12", ",", "1"}], "}"}]}], 
                    "]"}]}], ")"}]}]}], ")"}]}]}], ")"}]}]}]}], ")"}], "*", 
             RowBox[{"Sin", "[", 
              RowBox[{
               RowBox[{"360", "/", "N"}], "*", "i", " ", "Degree"}], "]"}]}], 
            ",", "j"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "0", ",", 
            RowBox[{"N", "-", "1"}]}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"j", ",", "0", ",", "h", ",", "h"}], "}"}]}], "]"}], ",", 
        "1"}], "]"}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"x", ",", "y", ",", "0"}], "}"}]}], "]"}]}], ";"}]], "Input",Exp\
ressionUUID->"6f9e55a5-f87e-4589-8750-28fd7552dbdf"]
}, Open  ]],

